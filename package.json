{
    "name": "template-vue",
    "description": "A Phaser 3 project template that demonstrates Vue communication and uses Vite for bundling.",
    "version": "1.1.2",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PrinceMagbanua/tile-matching.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/PrinceMagbanua/tile-matching/issues"
    },
    "homepage": "https://github.com/PrinceMagbanua/tile-matching",
    "keywords": [
        "phaser",
        "phaser3",
        "vue",
        "vite"
    ],
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "deploy": "gh-pages -d dist",
        "build:deploy": "vite build --config vite/config.prod.mjs && gh-pages -d dist"
    },
    "dependencies": {
        "phaser": "^3.80.1",
